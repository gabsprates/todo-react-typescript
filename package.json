{
  "name": "todo-react-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "test-watch": "npm test -- --watch",
    "build": "./node_modules/.bin/webpack",
    "watch": "npm run build -- --watch",
    "start": "./node_modules/.bin/webpack-dev-server --open --inline --output-public-path='/dist'"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [ "./enzyme.adapter.tsx" ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabsprates/todo-react-typescript.git"
  },
  "author": "Gabriel Prates <gabsprates@gmail.com> (http://gabrielprates.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabsprates/todo-react-typescript/issues"
  },
  "homepage": "https://github.com/gabsprates/todo-react-typescript#readme",
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.1.3",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.2",
    "react-test-renderer": "^16.2.0",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.0.4",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
